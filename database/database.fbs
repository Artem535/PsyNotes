table Note {
  id: ulong;
  visible: bool;

  title: string;
  /// objectbox:date
  time: ulong;
  /// objectbox:relation=NoteText
  noteTextId: ulong;

  emotState: byte;
  angryLevel: byte;
  sadLevel: byte;
  loveLevel: byte;
  fearLevel: byte;
  happyLevel: byte;
}

table NoteText {
  id: ulong;
  thoughtsTxt: string;
  behaviorTxt: string;
  bodyTxt: string;
  feelsTxt: string;
  situationTxt: string;
}

